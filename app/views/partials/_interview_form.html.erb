<%= form_with model: @interview, url: url, local: true do |form| %>
  <br><br><%= form.label "Date:" %>
  <%= form.date_field :date %>
  <%= form.label "Time:" %>
  <%= form.time_field :time %><br><br>
  <%= form.label "Stakeholder:" %><br>
  <%= form.text_field :stakeholder, size: "35x10" %><br><br>
  <%= form.label "Attendees:" %><br>
  <%= form.text_area :attendees, size: "30x10" %><br><br>
  <%= form.label "Background:" %><br>
  <%= form.text_area :background, size: "100x10" %><br><br>
  <%= form.label "Interview:" %><br>
  <%= form.text_area :interview, size: "100x10" %><br><br>
  <%= form.label "Report Summery:" %><br>
  <%= form.text_area :report_summery, size: "100x10" %><br><br>
  <%= form.submit %>
<% end %>
